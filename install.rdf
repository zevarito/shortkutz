    <!-- Front End MetaData -->

    <em:name>Shorkutz</em:name>
    <em:description>A access link extension</em:description>
    <em:creator>Alvaro Gil</em:creator>
    <em:homepageURL>http://localhost</em:homepageURL>
  </Description>
